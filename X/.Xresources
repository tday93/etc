! special 
*.foreground: #c5c8c6
*.background: #1d1f21
*.cursorColor: #c5c8c6
! Black + DarkGrey 
*color0:  #1d1f21
*color8:  #969896
! DarkRed + Red
*color1:  #cc6666
*color9:  #cc6666
! DarkGreen + Green
*color2:  #b5bd68
*color10: #b5bd68
! DarkYellow + Yellow
*color3:  #f0c674
*color11: #f0c674
! DarkBlue + Blue
*color4:  #81a2be
*color12: #81a2be
! DarkMagenta + Magenta
*color5:  #b294bb
*color13: #b294bb
!DarkCyan + Cyan (both not tango)
*color6:  #8abeb7
*color14: #8abeb7
! LightGrey + White
*color7:  #c5c8c6
*color15: #ffffff

Xft.dpi: 100
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight


URxvt*background: #010101
URxvt*foreground: #cccccc 
URxvt.scrollBar: false
URxvt*termName: rxvt-unicode-256color
URxvt.font: xft:Source Code Pro:size=10
URxvt.boldFont: xft:Source Code Pro:size=10
